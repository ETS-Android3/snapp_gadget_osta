<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profile_frag"
    android:background="@color/gray3">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">




    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">








        <include
            android:layout_width="match_parent"
            android:layout_height="?actionBarSize"
            layout="@layout/header_m_ac"
            android:id="@+id/header_ly_profile_frag_ly"/>










        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/margin_5"
            android:layout_marginRight="@dimen/margin_5"
            android:layout_marginTop="@dimen/margin_5"
            android:orientation="horizontal"
            android:layoutDirection="rtl"
            android:paddingRight="@dimen/margin_5"
            android:paddingTop="@dimen/margin_5"
            android:paddingLeft="@dimen/margin_5"
            android:id="@+id/profile_top_layout_profile_frag"
            android:layout_below="@id/header_ly_profile_frag_ly">






















            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_marginTop="@dimen/margin_5"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp">











                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/edittext_border"
                    android:onClick="onclick_items_profile_frag"
                    android:id="@+id/show_profile_item_profile_frag"
                    tools:ignore="OnClick">









                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="120dp">

                        <RelativeLayout
                            android:layout_width="120dp"
                            android:layout_height="120dp"
                            android:layout_centerHorizontal="true"
                            android:foreground="@drawable/oval_image_view_white_forground"
                            android:layout_margin="5dp">

                            <ImageView
                                android:layout_width="110dp"
                                android:layout_height="110dp"
                                android:src="@drawable/profile_image_top_resolution_icon"
                                android:layout_margin="14dp"
                                android:id="@+id/profile_image_view_profile_frag" />

                        </RelativeLayout>

                    </RelativeLayout>











                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:text="رضا فرازی"
                            android:textColor="@color/textcolor1"
                            android:id="@+id/name_and_family_textview_profile_frag"
                            android:layout_margin="2dp"
                            android:textSize="@dimen/text_view_text_size14"
                            android:fontFamily="@font/is1"/>




                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:text="091000000000"
                            android:textColor="#000"
                            android:layout_below="@id/name_and_family_textview_profile_frag"
                            android:layout_margin="2dp"
                            android:textSize="@dimen/text_view_text_size12"
                            android:id="@+id/phone_number_textview_profile_frag"
                            android:fontFamily="@font/is"/>




                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:text="@string/login_to_profile"
                            android:textColor="@color/colorPrimaryDark"
                            android:layout_below="@id/phone_number_textview_profile_frag"
                            android:layout_margin="2dp"
                            android:textSize="@dimen/text_view_text_size12"
                            android:fontFamily="@font/is"/>





                    </RelativeLayout>



                </LinearLayout>







                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/edittext_border"
                    android:layout_marginTop="@dimen/margin_5"
                    android:onClick="onclick_cell_to_backup"
                    tools:ignore="OnClick">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:text="@string/call_to_backup"
                            android:textColor="@color/textcolor1"
                            android:layout_margin="@dimen/margin_5"
                            android:textSize="@dimen/text_view_text_size14"
                            android:drawableRight="@drawable/phone_big_icon"
                            android:padding="17dp"/>


                    </RelativeLayout>



                </LinearLayout>









            </LinearLayout>


























            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_marginTop="@dimen/margin_5"
                android:layout_marginLeft="2.5dp"
                android:layout_marginRight="2.5dp">







                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/edittext_border"
                    tools:ignore="OnClick">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:text="@string/your_eshtrak"
                            android:textColor="@color/textcolor1"
                            android:layout_margin="2dp"
                            android:textSize="@dimen/text_view_text_size14"
                            android:id="@+id/eshtrak_text_view_profile_frag"
                            android:fontFamily="@font/is1"/>


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:text="0 هزار تومان"
                            android:textColor="@color/colorPrimaryDark"
                            android:layout_margin="2dp"
                            android:textSize="10dp"
                            android:layout_below="@+id/eshtrak_text_view_profile_frag"
                            android:id="@+id/eshtrak_total_text_view_profile_frag"
                            android:fontFamily="@font/is1"/>


                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/eshtrak_total_text_view_profile_frag"
                            android:orientation="horizontal"
                            android:layout_centerHorizontal="true"
                            android:id="@+id/end_ly_my_profile_frag"
                            android:textDirection="rtl"
                            android:layoutDirection="rtl">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/subscription_count"
                                android:textColor="#1d7636"
                                android:layout_margin="2dp"
                                android:textSize="12dp"
                                android:fontFamily="@font/is1"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0"
                                android:textColor="#1d7636"
                                android:layout_margin="2dp"
                                android:textSize="12dp"
                                android:id="@+id/eshtrak_all_propos_text_view_profile_frag"
                                android:fontFamily="@font/is1"/>


                        </LinearLayout>




                        <Button
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_centerHorizontal="true"
                            android:text="@string/price_n_props"
                            android:textColor="@color/colorAccent"
                            android:layout_margin="5dp"
                            android:textSize="@dimen/text_view_text_size14"
                            android:layout_below="@+id/end_ly_my_profile_frag"
                            android:id="@+id/eshtrak_price_button_profile_frag"
                            android:fontFamily="@font/is1"
                            android:background="@drawable/green_fosfor_btn_background"
                            android:onClick="onclick_new_sharj_profile_frag"/>



                    </RelativeLayout>



                </LinearLayout>










                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/edittext_border"
                    android:layout_marginTop="@dimen/margin_5"
                    tools:ignore="OnClick"
                    android:onClick="onclick_exit_from_account">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:text="@string/exit_from_acc"
                            android:textColor="@color/textcolor1"
                            android:layout_margin="2dp"
                            android:textSize="12dp"
                            android:fontFamily="@font/is1"
                            android:padding="14dp"/>



                    </RelativeLayout>



                </LinearLayout>







                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:background="@drawable/edittext_border"
                    android:layout_marginTop="@dimen/margin_5"
                    tools:ignore="OnClick">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">


                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:text="@string/date_create_in_snapp"
                            android:textColor="@color/textcolor1"
                            android:layout_margin="2dp"
                            android:textSize="12dp"
                            android:id="@+id/date_create_in_snapp_gadget_text_view_profile_frag"
                            android:fontFamily="@font/is1"
                            android:padding="4dp"/>




                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:text="@string/sharj_ing"
                            android:textColor="@color/colorPrimaryDark"
                            android:layout_margin="5dp"
                            android:textSize="@dimen/text_view_text_size14"
                            android:layout_below="@id/date_create_in_snapp_gadget_text_view_profile_frag"
                            android:fontFamily="@font/is1"
                            android:id="@+id/datecreate_text_view_profile_frag"
                            android:padding="4dp"/>





                    </RelativeLayout>



                </LinearLayout>





























            </LinearLayout>







        </LinearLayout>














        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="160dp"
            android:padding="@dimen/margin_5"
            android:layout_marginLeft="@dimen/margin_5"
            android:layout_marginRight="@dimen/margin_5"
            android:layout_below="@id/profile_top_layout_profile_frag"
            android:id="@+id/center_layout_profile_frag"
            android:orientation="horizontal"
            android:layoutDirection="rtl">






            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/edittext_border"
                android:layout_marginRight="2.5dp"
                android:layout_marginLeft="2.5dp"
                android:onClick="onclick_items_profile_frag"
                tools:ignore="OnClick"
                android:id="@+id/my_jobs_profile_frag">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_centerInParent="true"
                    android:drawableTop="@drawable/setting_icon_color"
                    android:text="@string/my_jobs"
                    android:textColor="@color/textcolor1"
                    android:textAlignment="center"
                    android:textSize="@dimen/button_text_text_size14"
                    android:layout_margin="@dimen/margin_5"/>


            </RelativeLayout>











            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="0.5"
                android:background="@drawable/edittext_border"
                android:layout_marginRight="2.5dp"
                android:layout_marginLeft="2.5dp">



                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_centerHorizontal="true"
                    android:layout_margin="@dimen/margin_5"
                    android:src="@drawable/proposal"
                    android:id="@+id/proposal_icon_image_view_profile_frag"/>



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_below="@+id/proposal_icon_image_view_profile_frag"
                    android:layout_margin="5dp">



                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:text="@string/totoal_count_sended_props"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="10dp"
                                android:fontFamily="@font/is1"
                                android:layout_margin="2dp"/>


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:text="0"
                                android:textColor="@color/textcolor1"
                                android:textSize="10dp"
                                android:fontFamily="@font/is1"
                                android:layout_margin="2dp"
                                android:id="@+id/my_all_props_count_text_view_profile_frag"/>



                        </LinearLayout>

                    </RelativeLayout>



                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:text="@string/count_sended_props"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="10dp"
                                android:fontFamily="@font/is1"
                                android:layout_margin="2dp"/>


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:text="0"
                                android:textColor="@color/textcolor1"
                                android:textSize="10dp"
                                android:fontFamily="@font/is1"
                                android:layout_margin="2dp"
                                android:id="@+id/my_spicial_props_count_text_view_profile_frag"/>



                        </LinearLayout>

                    </RelativeLayout>


                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:orientation="horizontal">


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:text="@string/spicial_count_sended_props"
                                android:textColor="@color/colorPrimaryDark"
                                android:textSize="10dp"
                                android:fontFamily="@font/is1"
                                android:layout_margin="2dp"/>


                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:text="0"
                                android:textColor="@color/textcolor1"
                                android:textSize="10dp"
                                android:fontFamily="@font/is1"
                                android:layout_margin="2dp"
                                android:id="@+id/my_my_props_count_text_view_profile_frag"/>



                        </LinearLayout>

                    </RelativeLayout>



                </LinearLayout>





            </RelativeLayout>















        </LinearLayout>

















        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/margin_5"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:layout_below="@id/center_layout_profile_frag"
            android:id="@+id/end_layout_profile_frag"
            android:orientation="vertical"
            android:layoutDirection="rtl"
            android:background="@drawable/edittext_border">





            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:text="@string/notifications"
                    android:textColor="#52ce30"
                    android:textSize="14dp"
                    android:fontFamily="@font/is1"
                    android:layout_margin="2dp"
                    android:drawableRight="@drawable/notification_icon"/>



            </RelativeLayout>





            <GridView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="5dp"
                android:id="@+id/notifications_grid_profile_frag"/>






        </LinearLayout>





















        </RelativeLayout>

    </ScrollView>


</RelativeLayout>