<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".white_for_frist_start_ac"
    android:background="#FFF">


    <ImageView
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/logo_s"
        android:layout_marginTop="150dp"
        android:id="@+id/image_view_white_for_frist_start"/>



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/image_view_white_for_frist_start"
        android:textColor="#222"
        android:layout_marginTop="25dp"
        android:text="@string/wite"
        android:textSize="@dimen/margin_15"
        android:layout_centerVertical="true"
        android:layout_centerInParent="true"
        android:id="@+id/text_view_white_for_frist_start"/>


    <ProgressBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/text_view_white_for_frist_start"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"/>




</RelativeLayout>